<p id="notice"><%= notice %></p>

<h1>Listing Teams</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>External</th>
      <th>Short name</th>
      <th>Name</th>
      <th>String</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= team.code %></td>
        <td><%= team.external_id %></td>
        <td><%= team.short_name %></td>
        <td><%= team.name %></td>
        <td><%= team.string %></td>
        <td><%= link_to 'Show', team %></td>
        <td><%= link_to 'Edit', edit_team_path(team) %></td>
        <td><%= link_to 'Destroy', team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team', new_team_path %>
<br>
<%= link_to 'Import Static Teams', importteams_path, method: :populate, data: { confirm: 'Are you sure?' } %>